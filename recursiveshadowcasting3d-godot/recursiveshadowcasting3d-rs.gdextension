[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../recursiveshadowcasting3d-rs/target/debug/librecursiveshadowcasting3d_rs.so"
linux.release.x86_64 =   "res://../recursiveshadowcasting3d-rs/target/release/librecursiveshadowcasting3d_rs.so"
